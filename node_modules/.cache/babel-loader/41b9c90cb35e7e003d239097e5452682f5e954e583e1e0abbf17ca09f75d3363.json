src=\"post.photo\"\r\n                class=\"post-image\"\r\n                alt=\"post_image\"\r\n            />\r\n          </div>\r\n          </div>\r\n\r\n          <div class=\"post-contentItem\">{{post.body}}</div>\r\n          <div class=\"post-bottom\">\r\n            <div class=\"post-profile\">\r\n              \r\n              \r\n              <div class=\"thumbs\">\r\n                <button v-on:click=\"addlike\" id = \"{{$post_id}}\"><img thumb src=\"../assets/images/like_thumb.png\" width=\" 20\" height=\" 20\"/></button>\r\n                {{ post.num_likes }} likes\r\n                </div>\r\n\r\n\r\n            </div>\r\n            \r\n          </div>\r\n        </div>\r\n      <!-- <p>  <b> Body: </b> {{post.body}} </p> -->\r\n    </div>\r\n    <div class=\"reset\">\r\n      <button v-on:click=\"zero\">Reset likes</button>\r\n</div> \r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n    name: \"getPost\",\r\n    methods: {\r\n      getimgURL: function (url) {\r\n        if (url!=\" \") {\r\n          return url;\r\n        }\r\n\r\n      },\r\n      addlike: function(){\r\n        window.alert(postid)\r\n        this.$store.commit(\"addlike\")\r\n        \r\n      },\r\n      \r\n      zero: function(){\r\n        this.$store.commit(\"zero\")\r\n    }\r\n    },\r\n    computed: {\r\n      \r\n      posts () { \r\n        \r\n  return this.$store.getters.postsget}\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n.reset{\r\n  text-align:center;\r\n  width: 75px;\r\n  height: 20px;\r\n  margin: 0 auto;\r\n  }\r\n\r\nbody {\r\n    line-height: 1.6;\r\n    font-size: 600px;\r\n}\r\n.post {\r\n  padding: 16px;\r\n  border-radius: 12px;\r\n  background-color: #A9A9A9;\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.post-header {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  gap: 16px;\r\n  margin-bottom: 8px;\r\n}\r\n\r\n.post-bottom {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  gap: 16px;\r\n  margin-top: 8px;\r\n}\r\n\r\n\r\n.post-profile {\r\n  flex: 0 0 auto;\r\n}\r\n\r\n.profilePlate-image {\r\n  display: inline-block;\r\n  height: 40px;\r\n  width: 40px;\r\n  object-fit: cover; /* to avoid image distortion */\r\n  vertical-align: top;\r\n}\r\n\r\n.post-image {\r\n  display: inline-block;\r\n  width: 100%;\r\n  vertical-align: top;\r\n  margin-bottom: 8px;\r\n}\r\n\r\n.post-image-like {\r\n  display: inline-block;\r\n  width: 10%;\r\n  vertical-align: bottom;\r\n}\r\n\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}